@model IReadOnlyList<TicketDto>

@{
    ViewData["Title"] = "Tickets";
}

<h1>Tickets</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Trailer Id</th>
            <th>Reported At</th>
            <th>Fixed At</th>
            <th>Report Note</th>
            <th>Fix Note</th>
            <th>Author Id</th>
            <th>Mechanic Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
        {
            <tr>
                <td>@t.TrailerId</td>
                <td>@t.ReportedAt</td>
                <td>@t.FixedAt</td>
                <td>@t.ReportNote</td>
                <td>@t.FixNote</td>
                <td>@t.AuthorId</td>
                <td>@t.MechanicId</td>
            </tr>
        }
</table>
